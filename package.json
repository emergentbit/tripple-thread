{
  "name": "@emergentbit/tripple-thread",
  "version": "0.3.2",
  "description": "A semantic data graph system using RDF and SQLite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "ts-node src/index.ts",
    "prepublishOnly": "npm run test && npm run build",
    "prepare": "npm run build",
    "preversion": "npm run test",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "rdf",
    "semantic-web",
    "sqlite",
    "graph-database",
    "typescript",
    "database",
    "triple-store"
  ],
  "author": "krispy@emergentbit.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emergentbit/tripple-thread.git"
  },
  "bugs": {
    "url": "https://github.com/emergentbit/tripple-thread/issues"
  },
  "homepage": "https://github.com/emergentbit/tripple-thread#readme",
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "rdf-data-factory": "^1.1.2",
    "rdf-parse": "^2.3.2",
    "rdf-serialize": "^2.2.2",
    "stream-to-string": "^1.2.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/sqlite3": "^5.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
